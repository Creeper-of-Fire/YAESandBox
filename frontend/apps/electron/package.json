{
  "name": "@yaesandbox-frontend/electron",
  "version": "1.0.0",
  "private": true,
  "description": "Electron wrapper and packager for YAESandBox",
  "author": "Creeper_of_Fire",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "build-sources": "node ./scripts/build.mjs",
    "assemble-app": "node ./scripts/assemble.mjs",
    "package:full": "pnpm build-sources && pnpm assemble-app && electron-builder",
    "package:fast": "pnpm assemble-app && electron-builder"
  },
  "dependencies": {
  },
  "build": {
    "appId": "com.creeperoffire.yaesandbox",
    "copyright": "Copyright Â© 2025 Creeper_of_Fire",
    "directories": {
      "output": "../../../build/installers",
      "app": "../../../build/app",
      "buildResources": "buildResources"
    },
    "files": [
      "**/*"
    ],
    "asar": true,
    "asarUnpack": [
      "**/*.exe",
      "**/*.dll",
      "**/*.pdb",
      "**/runtimes/**",
      "**/liblua54.framework/**",
      "Plugins/**/*"
    ],
    "win": {
      "target": [
        "dir"
      ],
      "icon": "./buildResources/vue.svg"
    }
  },
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1"
  }
}