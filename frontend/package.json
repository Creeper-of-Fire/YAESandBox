{
  "name": "yaesandbox-frontend",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @yaesandbox-frontend/shell dev",
    "preview": "pnpm --filter @yaesandbox-frontend/shell preview",
    "build": "pnpm --no-bail --filter \"./**\" build",
    "checkTsc": "pnpm --no-bail --filter \"./**\" checkTsc",
    "install:all": "pnpm install",
    "generate:licenses": "pnpm-licenses generate-disclaimer --prod --output-file=./apps/shell/public/THIRD_PARTY_LICENSES.txt",
    "check:licenses": "node ./scripts/check-licenses.mjs",
    "check:deps": "node ./scripts/check-deps.mjs",
    "build:app": "pnpm --filter @yaesandbox-frontend/shell build",
    "release": "python ./../scripts/release_helper.py"
  },
  "devDependencies": {
    "@quantco/pnpm-licenses": "^2.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/devtools": "^8.0.2",
    "@vue/tsconfig": "^0.7.0",
    "depcheck": "^1.4.7",
    "typescript": "latest",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "7.1.6",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "overrides": {
      "monaco-editor": "0.52.2"
    }
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
